using System;

namespace ConsoleApp1
{
    internal class Program
    {
        static void Main(string[] args)
        {
            //PART1
            
            //convert from Fahrenheit to Celsius

            Console.WriteLine("Temperture in Farenheit: ");

            int userInput1 = int.Parse(Console.ReadLine());

            int result1 = (userInput1 - 32) * 5/9;

            Console.WriteLine("Result: " + result1 + " Celsius");

            //convert Celsius to Fahrenheit:

            Console.WriteLine("Temperture in Celsius: ");

            int userInput2 = int.Parse(Console.ReadLine());

            int result2 = (userInput2* 9/5) + 32;

            Console.WriteLine("Result: " + result2 + " Farenheit");
            
            //PART2
            
            //convert from Fahrenheit to Celsius

            int originalTemperature = 0;

            print ("Fahrenheit: "+originalTemperature);

            int result1 = (originalTemperature - 32) * 5 / 9;

            print ("FahrenToCel: " + result1 + " Celsius");

            int result2 = (result1 * 9 / 5) + 32;

            print("CelToFahren: " + result2 + " Fahrenheit");

            //declaring three floas

            float originalTemperature2 = 0;

            print("Fahrenheit: " + originalTemperature2);

            float result3 = (originalTemperature2 - 32) * 5 / 9;

            print("FahrenToCel: " + result3 + " Celsius");

            float result4 = (result3 * 9 / 5) + 32;

            print("CelToFahren: " + result4 + " Fahrenheit");

            //declare three dounle variables

            double originalTemperature3 = 0;
    
            print("Fahrenheit: " + originalTemperature3);

            double result5 = (originalTemperature3 - 32) * 5 / 9;

            print("FahrenToCel: " + result5 + " Celsius");

            double result6 = (result5 * 9 / 5) + 32;

            print("CelToFahren: " + result6 + " Fahrenheit");
        }
    }
}
